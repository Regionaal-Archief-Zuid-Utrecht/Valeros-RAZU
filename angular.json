{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Valeros": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "css"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/Valeros",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "css",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/ng2-pdfjs-viewer/pdfjs",
                "output": "/assets/pdfjs"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@arcgis/core/assets",
                "output": "/assets/arcgis"
              }
            ],
            "styles": [
              "src/styles.css",
              "src/theme.css",
              "node_modules/ngx-float-ui/css/theme-white.css",
              "node_modules/@arcgis/core/assets/esri/themes/light/main.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "externalDependencies": [
                "http",
                "https",
                "url",
                "util",
                "net"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "6kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/app/config/razu-settings/settings/endpoints/razu-endpoint.settings.ts",
                  "with": "src/app/config/razu-settings/settings/endpoints/razu-endpoint.production.settings.ts"
                }
              ],
              "allowedCommonJsDependencies": [
                "normalize-url"
              ]
            },
            "acceptance": {
              "externalDependencies": [
                "http",
                "https",
                "url",
                "util",
                "net"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/app/config/razu-settings/settings/endpoints/razu-endpoint.settings.ts",
                  "with": "src/app/config/razu-settings/settings/endpoints/razu-endpoint.acceptance.settings.ts"
                }
              ]
            },
            "test": {
              "externalDependencies": [
                "http",
                "https",
                "url",
                "util",
                "net"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/app/config/razu-settings/settings/endpoints/razu-endpoint.settings.ts",
                  "with": "src/app/config/razu-settings/settings/endpoints/razu-endpoint.test.settings.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "allowedCommonJsDependencies": [
                "normalize-url"
              ]
            },
            "dev-build": {
              "externalDependencies": [
                "http",
                "https",
                "url",
                "util",
                "net"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "6kb"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Valeros:build:production"
            },
            "development": {
              "buildTarget": "Valeros:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "Valeros:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "css",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/ng2-pdfjs-viewer/pdfjs",
                "output": "/assets/pdfjs"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@arcgis/core/assets",
                "output": "/assets/arcgis"
              }
            ],
            "styles": [
              "src/styles.css",
              "src/theme.css",
              "node_modules/ngx-float-ui/css/theme-white.css",
              "node_modules/@arcgis/core/assets/esri/themes/light/main.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}